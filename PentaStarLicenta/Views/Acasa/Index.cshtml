<section id="contact">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-8 text-center">
                <h2 class="mt-0">Solicita o rezervare!</h2>
                <hr class="divider my-4">
                <p class="text-muted mb-5">Ne puteti contacta telefonic sau puteti solicita o rezervare prin formularul de mai jos.</p>
            </div>
        </div>
        <form>
            <div class="form-row">
                <div class="col-md-4 mb-4">
                    <label for="validationDefault01">Nume</label>
                    <input type="text" class="form-control" id="validationDefault01" placeholder="Nume" data-bind="textInput: newClientLastName" required>
                </div>
                <div class="col-md-4 mb-4">
                    <label for="validationDefault02">Prenume</label>
                    <input type="text" class="form-control" id="validationDefault02" placeholder="Prenume" required>
                </div>
                <div class="col-md-4 mb-4">
                    <label for="validationDefaultUsername">CNP</label>
                    <div class="input-group">
                        <input type="text" class="form-control" id="validationDefaultUsername" placeholder="CNP" aria-describedby="inputGroupPrepend2" required>
                    </div>
                </div>
            </div>
            <div class="form-row">
                <div class="col-md-4 mb-4">
                    <label for="validationDefault03">Seria & Nr</label>
                    <input type="text" class="form-control" id="validationDefault03" placeholder="Seria & Nr" required>
                </div>
                <div class="col-md-4 mb-4">
                    <label for="validationDefault04">Email</label>
                    <input type="text" class="form-control" id="validationDefault04" placeholder="Email" required>
                </div>
                <div class="col-md-4 mb-4">
                    <label for="validationDefault05">Telefon</label>
                    <input type="text" class="form-control" id="validationDefault05" placeholder="Telefon" required>
                </div>
            </div>
            <div class="form-row">
                <div class="col-md-4 mb-4">
                    <label for="validationDefault04">Tara</label>
                    <input type="text" class="form-control" id="validationDefault04" placeholder="Tara" required>
                </div>
                <div class="col-md-4 mb-4">
                    <label for="validationDefault05">Data Sosire</label>
                    <input type="text" class="form-control" id="validationDefault05" placeholder="Data Sosire" required>
                </div>
                <div class="col-md-4 mb-4">
                    <label for="validationDefault03">Data Plecare</label>
                    <input type="text" class="form-control" id="validationDefault03" placeholder="Data Plecare" required>
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-4">
                        <label for="validationDefault03">Tip Camera</label>
                        <input type="text" class="form-control" id="validationDefault03" placeholder="Tip Camera" required>
                    </div>
                    <div class="col-4">
                        <label for="validationDefault03">Camera</label>
                        <input type="text" class="form-control" id="validationDefault03" placeholder="Camera" required>
                    </div>
                    <div class="col-4 send-button ">
                        <button class="btn btn-success form-control" type="submit">Trimite</button>
                    </div>
                </div>
            </div>
        </form>
    </div>
</section>

