@model IEnumerable<PentaStarLicenta.DAL.Models.AccomodationRequest>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.OccupationDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ReleaseDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.OccupationDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ReleaseDate)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AccomodationRequestId }) |
            @Html.ActionLink("Details", "Details", new { id=item.AccomodationRequestId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.AccomodationRequestId })
        </td>
    </tr>
}

</table>
