<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - PentaStar</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/bundles/style/application")
    @Scripts.Render("~/bundles/modernizr")
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.3/css/all.css" integrity="sha384-UHRtZLI+pbxtHCWp1t77Bi1L4ZtiqrqD80Kn4Z8NTSRyMA2Fd33n5dQ8lWUE00s/" crossorigin="anonymous">



</head>
<body>

    @RenderBody()


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <script src="~/Scripts/Vendors/knockout-3.4.2.js"></script>
    <script src="~/Scripts/Vendors/jquery-ui.min.js"></script>
    <script src="//ajax.aspnetcdn.com/ajax/knockout/knockout-2.2.1.js"></script>
    <script src="~/Scripts/Vendors/knockout-jqueryui.min.js"></script>
    <script src="~/Scripts/Vendors/knockout.validation.js"></script>
    <script src="~/Scripts/Vendors/require.js"></script>
    @Scripts.Render("~/bundles/Application")
    <script>
        startApplication();
    </script>
</body>
</html>
