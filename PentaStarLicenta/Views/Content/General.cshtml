<div id="general" data-bind="visible: isViewVisible">
    <div class="row row-general-numbers">
        <div class="col-lg-3 col-md-6">
            <div class="card">
                <div class="card-body align-middle">
                    <div class="stat-widget-five">
                        <div class="stat-icon dib flat-color-1">
                            <i class="fas fa-money-bill-wave-alt"></i>
                        </div>
                        <div class="stat-content">
                            <div class="text-left dib">
                                <div class="stat-text"><span data-bind="text: pricesSum"></span></div>
                                <div class="stat-heading">Venit (RON)</div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-lg-3 col-md-6">
            <div class="card">
                <div class="card-body">
                    <div class="stat-widget-five">
                        <div class="stat-icon dib flat-color-2">
                            <i class="fas fa-users"></i>
                        </div>
                        <div class="stat-content">
                            <div class="text-left dib">
                                <div class="stat-text"><span data-bind="text: numberOfClients"></span></div>
                                <div class="stat-heading">Clienti</div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-lg-3 col-md-6">
            <div class="card">
                <div class="card-body">
                    <div class="stat-widget-five">
                        <div class="stat-icon dib flat-color-3">
                            <i class="fas fa-user-check"></i>
                        </div>
                        <div class="stat-content">
                            <div class="text-left dib">
                                <div class="stat-text"><span data-bind="text: numberOfEmployees"></span></div>
                                <div class="stat-heading">Angajati</div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-lg-3 col-md-6">
            <div class="card">
                <div class="card-body">
                    <div class="stat-widget-five">
                        <div class="stat-icon dib flat-color-4">
                            <i class="fas fa-door-open"></i>
                        </div>
                        <div class="stat-content">
                            <div class="text-left dib">
                                <div class="stat-text"><span data-bind="text: numberOfRooms"></span></div>
                                <div class="stat-heading">Camere</div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-lg-6">
            <div id="chartBars" style="min-width: 310px; height: 400px; margin: 0 auto"></div>
        </div>

        <div class="col-lg-6">

            <div id="chartLine" style="min-width: 310px; max-width: 800px; height: 400px; margin: 0 auto"></div>

        </div>
    </div>

    <!-- Orders -->
    <div class="orders">
        <div class="row">
            <div class="col-lg-8">
                <div class="card">
                    <div class="card-body">
                        <div class="row">
                            <h4 class="box-title">Rezervari recente </h4>
                            <ul class="pagination ml-auto">
                                <li data-bind="css: { disabled: !accomodations.hasPreviousPage() }" class="previous disabled">
                                    <a class="page-link" href="#" aria-label="Previous" data-bind="click: accomodations.toPreviousPage">
                                        <span aria-hidden="true">&laquo;</span>
                                        <span class="sr-only">Previous</span>
                                    </a>
                                </li>

                                <!-- ko foreach: accomodations.pages -->
                                <li data-bind="css: { active: $data == $parent.accomodations.pageNumber() }">
                                    <a class="page-link" href="#" data-bind="text: $data, click: $parent.accomodations.pageNumber.bind($data)"></a>
                                </li>
                                <!-- /ko -->

                                <li data-bind="css: { disabled: accomodations.isLastPage() }" class="next disabled page-item">
                                    <a class="page-link" data-bind="click: accomodations.toLastPage" href="#" aria-label="Next">
                                        <span aria-hidden="true">&raquo;</span>
                                        <span class="sr-only">Next</span>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <div class="card-body--">
                        <div class="table-stats order-table ov-h">
                            <table id="rooms-table" class="table">
                                <thead class="thead-light">
                                    <tr>
                                        <th scope="col">Data Sosire</th>
                                        <th scope="col">Data Plecare</th>
                                        <th scope="col">Client</th>
                                        <th scope="col">Camera</th>
                                    </tr>
                                </thead>
                                <tbody data-bind="foreach: accomodations.pageItems">
                                    <tr class="row-counter">
                                        <td data-bind="text: moment(OccupationDate).format('DD-MM-YYYY')"></td>
                                        <td data-bind="text: moment(ReleaseDate).format('DD-MM-YYYY')"></td>
                                        <td data-bind="text:  $parent.getClientFirstName(ClientId)"></td>
                                        <td data-bind="text:  $parent.getRoomName(RoomId)"></td>
                                        
                                    </tr>
                                </tbody>
                            </table>
                        </div> <!-- /.table-stats -->
                    </div>
                </div> <!-- /.card -->
            </div>  <!-- /.col-lg-8 -->

            <div class="col-lg-4">
                <div id="chartPie" style="min-width: 310px; height: 400px; max-width: 600px; margin: 0 auto"></div>
            </div> <!-- /.col-md-4 -->
        </div>
    </div>
    <!-- /.orders -->



</div>

